{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "create-env": {
      "inputs": [".env.example"],
      "outputs": [".env"]
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": ["tsconfig.json", "tsconfig.base.json", ".env"]
}
